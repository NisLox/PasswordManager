<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.PasswordManager"
            prefHeight="400.0" prefWidth="600.0">
    <GridPane fx:id="gridPane" hgap="20" vgap="10">
        <Label fx:id="welcomeNote" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
        <Label text="Website" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Label text="Username" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label text="Password" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Label fx:id="entryWebsite" text="entryWebsite" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <Label fx:id="entryUsername" text="entryUsername" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label fx:id="entryPassword" text="entryPassword" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
    </GridPane>
    <Button fx:id="addButton">Add +</Button>

</AnchorPane>
